<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://www.codeplex.com/prism"
        x:Class="AvonManager.Desktop.Shell"
        Title="Shell" Height="507" Width="872">
    <Window.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/AvonManager.Common;component/Assets/AvonMgrStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <TabControl Name="MainRegion" prism:RegionManager.RegionName="MainRegion"
                    ItemContainerStyle="{StaticResource RegionTabItemHeaderStyle}"/>
    </Grid>
</Window>
